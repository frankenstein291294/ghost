void main() {
  // por defecto dar nos da las lista como dinamica; lo que signica que la lista puede tener diferentes tipos de datos
  
  // para restringir los tipos de datos agregamos lo siguiente
  
  List<int> numero = [1,2,3,4,5];
  print(numero);
  numero.add(10);
  print(numero);
  
  
  List masNum = List(10);
  print(masNum);
  
  // masNum.add(1); esto no se puede hacer que es valor fijo
  
  masNum[0] = 10;
  print(masNum);
  
}
